import java.util.Scanner;

public class SearchArray {

    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);

        

        // Prompt the user to enter the size of the array

        System.out.print("Enter the size of the array: ");

        int size = input.nextInt();

        

        // Create an array of the specified size

        String[] array = new String[size];

        

        // Prompt the user to enter each element of the array

        for (int i = 0; i < size; i++) {

            System.out.print("Enter element " + (i+1) + ": ");

            array[i] = input.next();

        }

        

        // Prompt the user to enter the string to search for

        System.out.print("Enter a string to search for: ");

        String search = input.next();

        

        // Search for the string in the array

        boolean found = false;

        for (int i = 0; i < size; i++) {

            if (array[i].equals(search)) {

                found = true;

                break;

            }

        }

        

        // Output the result of the search

        if (found) {

            System.out.println(search + " was found in the array.");

        } else {

            System.out.println(search + " was not found in the array.");

        }

    }

}

 

 










      
   


